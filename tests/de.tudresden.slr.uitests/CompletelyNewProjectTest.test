Content-Type: text/ecl
Entry-Name: .content

get-menu -path "File/New/Other" | click
get-window New | get-tree | select "SLR/SLR Project" | double-click
with [get-window "New SLR Project"] {
    get-editbox -after [get-label "Project name:"] | set-text newProject
    with [get-button "Next >"] {
        click
        click
        click
    }
    get-button Finish | click
}
get-view "Project Explorer" | click
with [get-view "Project Explorer" | get-tree] {
    get-item -path newProject | get-property caption | equals newProject | verify-true
    get-item -path newProject | get-property selection | equals false | verify-true
    get-item -path newProject | get-property enablement | equals false | verify-true
    get-item -path newProject | get-property selected | equals false | verify-true
    get-item -path newProject | get-property checked | equals false | verify-true
}
with [get-view "Project Explorer" | get-tree] {
    get-item -path newProject | get-property "getClassName()" | equals "org.eclipse.swt.widgets.TreeItem" | verify-true
    get-item -path newProject | get-property "getItemCount()" | equals 3 | verify-true
    get-item -path newProject | get-property "getText()" | equals newProject | verify-true
    get-item -path newProject | get-property "getItems().getClassName()" | equals "org.eclipse.swt.widgets.TreeItem[]" 
        | verify-true
    get-item -path newProject | get-property "getItems().TreeItem[0].getClassName()" 
        | equals "org.eclipse.swt.widgets.TreeItem" | verify-true
    get-item -path newProject | get-property "getItems().TreeItem[0].getText()" | equals "my_bibtex.bib" | verify-true
    get-item -path newProject | get-property "getItems().TreeItem[1].getClassName()" 
        | equals "org.eclipse.swt.widgets.TreeItem" | verify-true
    get-item -path newProject | get-property "getItems().TreeItem[1].getText()" 
        | equals "my_metainformation.slrproject" | verify-true
    get-item -path newProject | get-property "getItems().TreeItem[2].getClassName()" 
        | equals "org.eclipse.swt.widgets.TreeItem" | verify-true
    get-item -path newProject | get-property "getItems().TreeItem[2].getText()" | equals "my_taxonomy.taxonomy" 
        | verify-true
}

get-view "Project Explorer" | get-tree | select "newProject/my_bibtex.bib" | double-click
with [get-editor "my_bibtex.bib" | get-text-viewer] {
    get-property className | equals "org.eclipse.swt.custom.StyledText" | verify-true
    get-property enablement | equals true | verify-true
    get-property containMenu | equals true | verify-true
    get-property readOnly | equals false | verify-true
    get-property text | equals "% This bibtex file was created by the new bibtex wizard" | verify-true
}
get-view "Project Explorer" | get-tree | select "newProject/my_metainformation.slrproject" | double-click
with [get-editor "my_metainformation.slrproject" | get-control Any -index 3] {
    get-property "getClassName()" | equals "org.eclipse.swt.widgets.Composite" | verify-true
    get-property "getChildren().getClassName()" | equals "org.eclipse.swt.widgets.Control[]" | verify-true
    get-property "getChildren().Control[0].getClassName()" | equals "org.eclipse.swt.widgets.Group" | verify-true
    get-property "getChildren().Control[0].getText()" | equals "Key facts" | verify-true
    get-property "getChildren().Control[0].getChildren().getClassName()" | equals "org.eclipse.swt.widgets.Control[]" 
        | verify-true
    get-property "getChildren().Control[0].getChildren().Control[0].getClassName()" 
        | equals "org.eclipse.swt.widgets.Label" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[1].getClassName()" 
        | equals "org.eclipse.swt.widgets.Text" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[2].getClassName()" 
        | equals "org.eclipse.swt.widgets.Label" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[3].getClassName()" 
        | equals "org.eclipse.swt.widgets.Text" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[4].getClassName()" 
        | equals "org.eclipse.swt.widgets.Label" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[5].getClassName()" 
        | equals "org.eclipse.swt.widgets.Text" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[6].getClassName()" 
        | equals "org.eclipse.swt.widgets.Group" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getClassName()" 
        | equals "org.eclipse.swt.widgets.Group" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getText()" | equals Authors | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getChildren().getClassName()" 
        | equals "org.eclipse.swt.widgets.Control[]" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getChildren().Control[0].getClassName()" 
        | equals "org.eclipse.swt.widgets.Button" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getChildren().Control[1].getClassName()" 
        | equals "org.eclipse.swt.widgets.Button" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getChildren().Control[2].getClassName()" 
        | equals "org.eclipse.swt.widgets.Button" | verify-true
    get-property "getChildren().Control[0].getChildren().Control[7].getChildren().Control[3].getClassName()" 
        | equals "org.eclipse.swt.widgets.List" | verify-true
    get-property "getLayout().getClassName()" | equals "org.eclipse.swt.layout.GridLayout" | verify-true
    get-property "getLayout().numColumns" | equals 1 | verify-true
}
get-view "Project Explorer" | get-tree | select "newProject/my_taxonomy.taxonomy" | get-menu 
    -path "Open With/Taxonomy Editor" | click
with [get-editor "my_taxonomy.taxonomy" | get-text-viewer] {
    get-property className | equals "org.eclipse.swt.custom.StyledText" | verify-true
    get-property enablement | equals true | verify-true
    get-property containMenu | equals true | verify-true
    get-property text | equals "/* This taxonomy was created by the new taxonomy wizard\n"
        + "   It holds a simple taxonomy to showcase the syntax */\n\n"
        + "Dimension 1 {\n"
        + "\tTerm 1 {\n"
        + "\t\tSubterm 1,\n"
        + "\t\tSubterm 2\n"
        + "\t},\n"
        + "\tTerm 2,\n"
        + "\tTerm 3\n"
        + "},\n\n"
        + "Dimension 2 {\n"
        + "\tTerm 1 {\n"
        + "\t\tSubterm 1,\n"
        + "\t\tSubterm 2\n"
        + "\t},\n"
        + "\tTerm 2,\n"
        + "\tTerm 3\n"
        + "}" | verify-true
}

