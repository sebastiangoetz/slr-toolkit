}

@InProceedings{Fratu2017,
	author = {Mariana Fratu and Lucian Cirstolovean},
	title = {Information modelling for energy efficiency in intelligent buildings},
	booktitle = {2017 International Conference on Optimization of Electrical and Electronic Equipment ({OPTIM}) {\&} 2017 Intl Aegean Conference on Electrical Machines and Power Electronics ({ACEMP})},
	year = {2017},
	month = "may",
	publisher = {{IEEE}},
	abstract = {In this article one examine the topic of Building Information Modeling (BIM) from the perspective of the residential building' owners. We present a novel approach to estimate of energy consumption through the identification of the relevant parameters and the application of Soft Computing techniques. The residential buildings must be capable of not only providing mechanisms to minimize their energy consumption, but also integrating their own energy sources to ensure their energy sustainability. We characterize the building in terms of its contextual features and energy consumption, and then select the most appropriate techniques to generate the most accurate model charged with energy rationalization. The innovative technologies would need to ensure that the owners achieve management tool to provide advanced energy management of intelligent built environment, where smart objects, as well as distributed energy resources are installed.},
	classes = {Veroeffentlichungsland{Rumaenien, Europa{Rumaenien}}Veroeffentlichungssort{International Aegean Conference on Electrical Machines and Power Electronics, International Conference on Optimization of Electrical and Electronic Equipment}Zielgruppe{Eigentuemer, Facility Manager, Privatsektor{Eigentuemer}}Art{Conference, Konferenz}Thema{Energieverbrauch von Gebaeuden mithilfe von BIM analysieren und minimieren, Anwendung von BIM auf intelligente Gebaeude, Nachhaltigkeit und Energieverbrauch, Gebaeudeanalyse Nachhaltigkeit und Energieverbrauch}Anwendungsfeld{Umwelt und Energie, Immobiliengeschaeft, Facility Management}Abstraktionsgrad{aktuell{Fallstudie}}Ergebnis{anwendbar{Implementation}, praktisch{Implementation}}Methodik{Entwurf, Bericht}},
	doi = {10.1109/optim.2017.7975087},
