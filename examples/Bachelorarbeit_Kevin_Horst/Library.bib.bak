	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\learning.pdf:PDF},
	keywords = {Architecture and Engineering Education, Augmented Reality, Teaching Tools, Building Information Modelling },
	url = {http://ieeexplore.ieee.org/document/7836510/}
}

@InProceedings{Zhao2016,
	author = {Qin Zhao and Zeyu Chen and Yiyun Zhu and Zhe Li and Xinhong Hei},
	title = {Toward Automatic Review of Building Energy Efficiency Based on Building Information Modeling},
	booktitle = {2016 12th International Conference on Computational Intelligence and Security ({CIS})},
	year = {2016},
	month = "dec",
	publisher = {{IEEE}},
	abstract = {Building energy consumption increases with the acceleration process of urban constructions. The city faces the problem of energy shortage, which leads to the building energy efficiency becoming an urgent need for the global environment. BIM (Building Information Modeling) technology has been widely used in the construction industry. The current building energy consumption calculation depending on that the BIM model is imported to different energy consumption software for energy consumption analysis. There are few methods to review building energy efficiency requirements automatically. In view of this, this paper discusses the method of review of building energy consumption in Revit platform. The feasibility of the method is proved through an office building construction. The method can automatically review whether design results of the building meet the building energy efficiency requirements in the design process and can shorten the review time.},
	classes = {Zielgruppe{unspezifisch}Veroeffentlichungsland{Asien{China}}Veroeffentlichungssort{International Conference on Compututational Intelligence and Security}Art{Conference, Konferenz}Thema{Nachhaltigkeit und Energieverbrauch, Gebaeudeanalyse Nachhaltigkeit und Energieverbrauch}Anwendungsfeld{AEC{Vorkonstruktion, Entwurf}, Organisation, Modellierung}Ergebnis{theoretisch{Workflow, Arbeitsfluss}}Methodik{Entwurf, Bericht}},
	doi = {10.1109/cis.2016.0057},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\energy_efficiency.pdf:PDF},
	keywords = {Heat transfer, Windows, Energy efficiency, Energy consumption, Shape, Floors },
	url = {http://ieeexplore.ieee.org/document/7820448/}
}

@InProceedings{Min2016,
	author = {Xu Min},
	title = {Study on Interior Design Based on Building Information Modeling},
	booktitle = {2016 International Conference on Smart City and Systems Engineering ({ICSCSE})},
