@Article{Shen2012,
	author = {Weilin Shen and Qiping Shen and Quanbin Sun},
	title = {Building Information Modeling-based user activity simulation and evaluation method for improving designer{\textendash}user communications},
	journal = {Automation in Construction},
	year = {2012},
	volume = {21},
	pages = {148--160},
	month = "jan",
	abstract = {There are continuous interactions between the designers and users during early architectural design stage. However, users’ limited comprehension in the three dimensional space may reduce the efficiency of requirement specification as well as design review process. This paper introduces a Building Information Modeling-based user activity simulation and evaluation method (UASEM), which aims to facilitate the designer-user communications. It assists users to specify their activities in new buildings and simulates these activities based on the building information models. It also helps to conduct a pre-occupancy evaluation. The UASEM was adopted in a case study of a university campus project. The result indicated that this method has potential to improve users’ understanding of the design solutions, especially in terms of spatial properties of the layout. It can also improve users’ confidence to express comments and increase their willingness to collaboratively work with designers.},
	classes = {Veroeffentlichungsland{China, UK, Asien{China}, Europa{UK, Vereinigtes Koenigreich}}Zielgruppe{Privatsektor{Modellierer, Architekten, Ingeneure, Konstrukteure, AEC-Hintergrund, Projektmanager}}Veroeffentlichungssort{Automation in Construction}Art{Journal, Fachzeitschrift}Thema{Interkommunikation und Interaktion von Benutzern von BIM}Anwendungsfeld{AEC, Modellierung, AEC{Vorkonstruktion, Entwurf, Konstruktion}, Organisation}Ergebnis{praktisch{Framework}}Methodik{Entwurf, Analyse, Diskussion}},
	doi = {10.1016/j.autcon.2011.05.022},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\activity_simulation_evaluation.pdf:PDF},
	keywords = {Building information model User activity simulation Pre-occupancy evaluation Architectural design},
	publisher = {Elsevier {BV}},
	url = {https://ac.els-cdn.com/S0926580511001117/1-s2.0-S0926580511001117-main.pdf?_tid=ee87caf0-cd6c-11e7-8f5a-00000aab0f27&acdnat=1511125520_3f8e8abf1c123d79498ffb99e56eaa3d}
}

@Article{Sacks2013,
	author = {R. Sacks and E. Pikas},
	title = {Building Information Modeling Education for Construction Engineering and Management. I: Industry Requirements, State of the Art, and Gap Analysis},
	journal = {Journal of Construction Engineering and Management},
	year = {2013},
	volume = {139},
	number = {11},
	pages = {04013016},
	month = "nov",
	abstract = {As building information modeling (BIM) becomes increasingly standard practice in the construction industry, universities providing construction engineering and management education seek to incorporate BIM concepts and skills in their degree programs. The goal of this research was to compile a framework for BIM education that lays out the necessary topics and the levels of achievement required at each stage of degree programs. Industry’s requirements for graduate engineers were elicited through surveys, workshops, analyses of job advertisements, and in-depth interviews. An outline of 39 topics in three broad classifications was established. Targets for competency in each topic were developed using the cognitive domain of Bloom’s taxonomy, which expresses the learning objectives on six levels (from understand to evaluate). Gap analysis, which compared the state of the art in leading universities to industry requirements, led to the compilation of a framework for the development of BIM content for undergraduate and graduate construction engineering and management degree programs. The requirements and framework are intended to provide educators with essential knowledge as they develop and implement BIM content in their programs.},
	classes = {Veroeffentlichungsland{Israel, Asien{Israel}}Methodik{Interviews, Umfrage, Analyse, Forschungs Workshop}Zielgruppe{Oeffentlicher Sektor{Studenten und Professoren}}Veroeffentlichungssort{Journal of Construction Engineering and Management}Art{Journal, Fachzeitschrift}Thema{Verbesserte Lernerfahrung durch BIM}Anwendungsfeld{AEC, unspezifisch}Ergebnis{theoretisch{Framework, Workflow, Arbeitsfluss}}},
	doi = {10.1061/(asce)co.1943-7862.0000759},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\learning_education.pdf:PDF},
	publisher = {American Society of Civil Engineers ({ASCE})}
}

@Article{Wang2013,
	author = {Xiangyu Wang and Peter E.D. Love and Mi Jeong Kim and Chan-Sik Park and Chun-Pong Sing and Lei Hou},
	title = {A conceptual framework for integrating building information modeling with augmented reality},
	journal = {Automation in Construction},
	year = {2013},
	volume = {34},
	pages = {37--44},
	month = "sep",
	abstract = {During the last two decades, designers have been embracing building information modeling (BIM) to im-
		prove the quality of the documentation that is produced as well as constructability. While BIM has become
		an innate feature of the design process within the construction industry, there have been limited investiga-
		tions that have examined how it can be integrated into real-time communication on-site. In addressing
		this gap, this paper proposes a conceptual framework that integrates BIM with augmented reality (AR) so
		as to enable the physical context of each construction activity or task to be visualized in real-time. To be ef-
		fective, it is suggested that AR should be ubiquitous (including context awareness) and thus operate in con-
		junction with tracking and sensing technologies such as radio frequency identification (RFID), laser pointing,
		sensors and motion tracking.},
	classes = {Thema{Echtzeitvisualisierung mit BIM und Augmented Reality}Veroeffentlichungssort{Automation in Construction}Art{Journal, Fachzeitschrift}Veroeffentlichungsland{Australien, Asien{Suedkorea}}Zielgruppe{Privatsektor{Projektmanager, AEC-Hintergrund, Facility Manager}}Anwendungsfeld{AEC{Vorkonstruktion, Entwurf, Konstruktion}, Facility Management}Ergebnis{theoretisch{Workflow, Arbeitsfluss}}Methodik{Entwurf, Bericht}},
	doi = {10.1016/j.autcon.2012.10.012},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\conceptual.pdf:PDF},
	keywords = {Augmented reality BIM Real-time visualization Tracking Sensing},
	publisher = {Elsevier {BV}},
	url = {https://ac.els-cdn.com/S0926580512001793/1-s2.0-S0926580512001793-main.pdf?_tid=6446b21a-cd6d-11e7-8f65-00000aab0f6c&acdnat=1511125726_2f7099304891962514d8803e897f9671}
}

@Article{Meza2014,
	author = {Sebastjan Me{\v{z}}a and {\v{Z}}iga Turk and Matev{\v{z}} Dolenc},
	title = {Component based engineering of a mobile {BIM}-based augmented reality system},
	journal = {Automation in Construction},
	year = {2014},
	volume = {42},
	pages = {1--12},
	month = "jun",
	abstract = {Over the last decade, building information models (BIM) have become increasingly popular. Yet their use on con-
		struction sites where the digital materializes, is limited. A technology that can bridge the gap between the digital
		and the real worldisaugmented reality (AR). Weanalyzethis gap and the AR potential and present how thecom-
		ponent based software engineering method can be used to efficiently implement a BIM-based AR system for con-
		struction. An architecture of the software system is proposed and verified by a prototype which was tested in a
		real construction project. We found out that the use of AR can significantly narrow the semantic gap between
		the digital model and the real world; that components for creating a BIM-based AR systems exist but currently
		do not scale well to large models; and that the use in AR applications creates additional requirements the for
		BIM models and tools, particularly related to the BIM's 4th dimension.},
