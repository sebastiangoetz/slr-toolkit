@Book{Eastman2008,
	title = {BIM Handbook - A Guide to Building Information Modeling for Owners, Managers, Designers, Engineers, and Contractors},
	publisher = {John Wiley \& Sons, Inc.},
	year = {2008},
	author = {Chuck Eastman, Paul Teicholz, Rafael Sacks, Kathleen Liston},
	address = {Corporate Headquarters 111 River Street Hoboken, NJ 07030-5774},
	isbn = {978-0-470-18528-5},
	abstract = {This book is about a new approach to design,  construction,  and facility management called building information modeling (BIM). It provides an in - depth understanding of BIM technologies, the business and organizational issues associated with its implementation, and the profound impacts that effective use of BIM can provide to all members of a project team. The book  explains how  designing, constructing,  and operating buildings with BIM differs from  pursuing  the  same  activities in the traditional way  using  drawings, whether paper or electronic. BIM is beginning to change the way buildings look, the way they function, and the ways in which they are built. Throughout the book, we have intentionally and consistently used the term ‘ BIM ’ to describe an activity (meaning building information modeling),  rather than an object (building information model).This reflects our belief that BIM is not a thing or a type of software but a human activity that ultimately involves broad process changes in construction. },
	classes = {Zielgruppe{Projektmanager, Eigentuemer, Ingeneure, Modellierer, Architekten, Konstrukteure, IT Entwickler, Facility Manager, Regierung, Studenten und Professoren, Privatsektor{Eigentuemer, Projektmanager, AEC-Hintergrund, Facility Manager}}Veroeffentlichungssort{John Wiley and Sons Inc}Art{Buch}Thema{Information und Anleitung zu BIM, Innovative Forschungsthemen und Trends, Uebersicht und Anleitung zu BIM}Anwendungsfeld{AEC, Ablaufplanung, Facility Management, Modellierung, AEC{Vorkonstruktion, Konstruktion}, Finanzierung, Organisation}Ergebnis{theoretisch{Ueberblick, Fallstudie}}Veroeffentlichungsland{USA, Kanada, Nordamerika{USA, Vereinigte Staaten von Amerika}}Methodik{Argumentation, Diskussion, Analyse, Bericht}},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\BIM_handbook_A_guide_to_building_informa.pdf:PDF},
	keywords = {Data processing, Building management,Communication in the building trades, Architectural practice,Architects and builders},
	url = {https://s3.amazonaws.com/academia.edu.documents/31207284/BIM_Handbook_1st.pdf?AWSAccessKeyId=AKIAIWOWYYGZ2Y53UL3A&Expires=1511104088&Signature=%2FZO1VgwCKrhskwsizdc9SoPFjgc%3D&response-content-disposition=inline%3B%20filename%3DBIM_handbook_A_guide_to_building_informa.pdf}
}

@TechReport{Azhar2011a,
  author      = {Salman Azhar, Michael Hein, Blake Sketo},
  title       = {Building Information Modeling (BIM): Benefits, Risks and Challenges},
  institution = {McWhorter School of Building Science Auburn University},
  year        = {2011},
  type        = {techreport},
  address     = {Auburn, Alabama 36849, USA},
  month       = {June},
  abstract    = {Building Information Modeling (BIM) has recently attained widespread attention in the Architectural, Engineering and Construction (AEC) industry. BIM represents the development and use of computergenerated n -dimensional (n - D) models to simulate the planning, design, construction and operation of a facility. It helps architects, engineers and constructors to visualize what is to be built in simulated environment and to identify potential design, construction or operational problems . In this paper, the benefits and possible risks of BIM and future challenges for the construction industry are discussed. First presented is the main concept of BIM with its advantages and possible applications in construction. Then the role of BIM in the construction industry and academia is discussed based on the results of three recent questionna ire surveys. After that, a case study of Hilton Aquarium project in Atlanta is presented to quantitatively illustrate the cost and time savings realized by developing and using a building information model.It is followed by data from 10 construction projects to determine the net BIM savings and BIM return on investment. At the end, BIM risks and future challenges for the construction industry are discussed.},
  classes     = {Veroeffentlichungsland{USA, Nordamerika{Vereinigte Staaten von Amerika}}Art{Journal, Fachzeitschrift}Veroeffentlichungssort{Leadership and Management in Engineering}Ergebnis{theoretisch{Fallstudie, Analyse, Ueberblick}}Methodik{Umfrage, Analyse, Argumentation, Diskussion}Zielgruppe{Eigentuemer, Ingeneure, Konstrukteure, Privatsektor{AEC-Hintergrund}}Thema{Qualitative und finanzielle Vorteile von BIM praesentieren}Anwendungsfeld{Finanzierung, Organisation}},
  file        = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\Techreport_Auburn_Azhar.pdf:PDF},
  keywords    = {Building Information Model (BIM), Virtual Design and Construction (VDC), n-Dimensional Modeling, Parametric Modeling, Facilities Management (FM)},
  url         = {http://ascpro.ascweb.org/chair/paper/CPGT182002008.pdf},
}

@Article{Lee2006,
	author = {Ghang Lee, Rafael Sacks, Charles M. Eastman},
	title = {Specifying parametric building object behavior (BOB) for a building information modeling system},
	journal = {Automation in Construction},
	year = {2006},
	volume = {15},
	number = {6},
	pages = {758-776},
	month = "November",
	abstract = {Parametric modeling has been proposed as an effective means to embed domain expertise in models of buildings. As information technology
		becomes more powerful in terms of the ability to manipulate large parametric models, the potential grows to build increasingly sophisticated
		functional systems for designing, modeling and fabricating buildings. Implementing more powerful systems implies greater functional
		specificity, which requires elicitation and capture of increasingly detailed and complex domain-specific semantics and knowledge. This paper
		explores the extent to which design and engineering knowledge can be practically embedded in production software for building information
		modeling (BIM). It focuses on a building object behavior (BOB) description notation and method, developed as a shorthand protocol for
		designing, validating and sharing the design intent of parametric objects. Examples are drawn from an advanced BIM system development
		project for precast concrete.},
	classes = {Veroeffentlichungssort{Automation in Construction}Veroeffentlichungsland{USA, Israel, Nordamerika{USA, Vereinigte Staaten von Amerika}, Asien{Israel}}Art{Journal, Fachzeitschrift}Thema{Informationsberechnung und Praesentation}Anwendungsfeld{Modellierung}Ergebnis{anwendbar{Implementation}, theoretisch{Workflow}, praktisch{Implementation}}Methodik{Entwurf, Bericht}Zielgruppe{Privatsektor{IT-Hintergrund}}},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\Specifying_parametric_building_object_be.pdf:PDF},
	howpublished = {Elsevier B.V.},
	keywords = {Parametric modeling, Building object behavior, Expert knowledge},
	url = {https://s3.amazonaws.com/academia.edu.documents/39529425/Specifying_parametric_building_object_be20151029-19842-ibt3xi.pdf?AWSAccessKeyId=AKIAIWOWYYGZ2Y53UL3A&Expires=1511109212&Signature=YL8FYeJ0Lb%2FSh8bM7bDi4ATqFs8%3D&response-content-disposition=inline%3B%20filename%3DSpecifying_parametric_building_object_be.pdf}
}

@Article{Azhar2011b,
	author = {Salman Azhar, Wade A. Carlton, Darren Olsen, Irtishad Ahmad},
	title = {Building information modeling for sustainable design and LEED ®rating analysis},
	journal = {Automation in Construction},
	year = {2011},
	volume = {20},
	number = {2},
	pages = {217-224},
	month = "March",
	abstract = {Today, there is a high level of demand for sustainable buildings. The most important decisions regarding a building's sustainable features are made during the design and preconstruction stages. Leadership in Energy and Environmental Design (LEED®) is the most widely adopted sustainable building rating system in the United States. For projects pursuing LEED®certification, designers have to conduct in-depth sustainability analyses based on a building's form, materials, context, and mechanical – electrical – plumbing (MEP) systems. Since Building Information Modeling (BIM) allows for multi-disciplinary information to be superimposed within one model, it creates an opportunity to conduct these analyses accurately and efficiently as compared to the traditional methods. In this exploratory research, a case study was conducted on Salisbury University's Perdue School of Business building to demonstrate the use of BIM for sustainable design and the LEED®certification process. First, a conceptual framework was developed to establish the relationship between BIM- based sustainability analyses and the LEED®certification process. Next, the framework was validated via this case study. The results of this study indicate that documentation supporting LEED®credits may be directly or indirectly prepared using the results of BIM-based sustainability analyses software. This process could streamline the LEED®certification process and save substantial time and resources which would otherwise be required using traditional methods. },
	classes = {Veroeffentlichungssort{Automation in Construction}Art{Journal, Fachzeitschrift}Thema{Energieverbrauch von Gebaeuden mithilfe von BIM analysieren und minimieren, Nachhaltigkeit und Energieverbrauch, Gebaeudeanalyse Nachhaltigkeit und Energieverbrauch}Anwendungsfeld{Umwelt und Energie, AEC{Vorkonstruktion, Entwurf, Konstruktion}}Ergebnis{theoretisch{Framework, Workflow, Analyse, Bericht}, praktisch{Fallstudie}}Methodik{Literaturrezension, Interviews, Entwurf, Fallstudie, Analyse, Bericht}Zielgruppe{Privatsektor{Projektmanager, AEC-Hintergrund}}Veroeffentlichungsland{Nordamerika{Vereinigte Staaten von Amerika}}},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\BIM_for_sustai_LEED.pdf:PDF},
	keywords = {BIM, Sustainable design, LEED® rating, Green building rating systems, Building performance analyses,},
	url = {https://s3.amazonaws.com/academia.edu.documents/50722176/Building_information_modeling_for_sustainable_design_and_LEED_rating_analysis.pdf?AWSAccessKeyId=AKIAIWOWYYGZ2Y53UL3A&Expires=1511110760&Signature=Pa%2BiCmvB%2FG8Z9znqBz1J1P8FU4Y%3D&response-content-disposition=inline%3B%20filename%3DBuilding_information_modeling_for_sustai.pdf}
}

@Article{Volk2014,
	author = {Volk, R.; Stengel, J.; Schultmann, F.},
	title = {Building Information Model ing (BIM) for existing buildings – literature review and future needs},
	journal = {Automation in Construction},
	year = {2014},
	volume = {38},
	pages = {109 -127},
	abstract = {While BIM processes are established for new buildings,  the majority of existing buildings  is not maintained, refurbished or deconstructed with BIM yet. Promising benefits  of efficient resource management motivate research to overcome uncertainties of building condition and deficient documentation prevalent in existing buildings.Due to rapid developments in BIM research, involved stakeholders demand a state-of- the-art  overview  of BIM implementation and research in existing buildings. This paper presents a review of over recent 180 publications on the topic.Results show scare BIM implementation in existing buildings yet, due to challenges of (1) high modeling  /  conversion effort from captured building data into semantic BIM objects, (2) updating of information in BIM and (3) handling of uncertaindata, objects and relations in BIM  occurring in existing buildings.  Despite fast developments and spreading s tandards, challenging research opportunities arise from process automation and BIM adaption to existing buildings’ requirements.  },
	classes = {Veroeffentlichungsland{Deutschland, Europa{Deutschland}}Veroeffentlichungssort{Automation in Construction}Art{Uebersichtsarbeit, Fachzeitschrift}Zielgruppe{unspezifisch}Anwendungsfeld{unspezifisch}Ergebnis{theoretisch{Ueberblick, Analyse}}Methodik{Literaturrezension, Analyse}Thema{Innovative Forschungsthemen und Trends, Anwendung von BIM auf bereits existierende Gebaeude}},
	doi = {10.1016/j.autcon.2013.10.023.},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\Volketal.2014_BIMforexistingbuildings_final_PostPrint_ok.pdf:PDF},
	institution = {Institute for Industrial Production (IIP), Karlsruhe Institute of Technology (KIT)},
	keywords = {As - built BIM, Existing buildings,Facility management , Maintenance, Retrofits , Deconstruction, Dismantling, Demolition, Scan - to - BIM , Reverse engineering },
	url = {https://www.researchgate.net/profile/Rebekka_Volk/publication/259518042_Building_Information_Modeling_BIM_for_existing_buildings_-_Literature_review_and_future_needs_Autom_Constr_38_March_2014_109-127/links/59db8b0a0f7e9b755ef7e9ec/Building-Information-Modeling-BIM-for-existing-buildings-Literature-review-and-future-needs-Autom-Constr-38-March-2014-109-127.pdf}
}

@Article{Goedert2008,
	author = {James D. Goedert and Pavan Meadati},
	title = {Integrating Construction Process Documentation into Building Information Modeling},
	journal = {Journal of Construction Engineering and Management},
	year = {2008},
	volume = {134},
	number = {7},
	pages = {509--516},
	month = "jul",
	abstract = {The research described in this paper extends the use of building information modeling(BIM) throughout the construction
		phase of the project life cycle. The owner is a military base that uses three-dimensional (3D) modeling for underground services and the
		footprints of the buildings. They were interested in determining the feasibility of capturing the construction process and related documents
		into a similar format. The research was conducted in parallel with traditional methods. The objectives of this project were to create a 3D
		as-built model, a four-dimensional as-built model, and attach the construction process information to the model for the owner to use after
		construction. A literature review indicates that BIM application stops at the preconstruction phase with a limited amount of research
		regarding data collection of the construction process. Significant contributions include practical 3D data collection methods and extending
		the BIM software products to accommodate construction process documentation. Results indicate that BIM software is not specifically
		prepared to accomplish these objectives and some modification to procedures as well as software were necessary for the BIM to capture
		the construction process documentation.},
	classes = {Veroeffentlichungsland{USA, Nordamerika{USA, Vereinigte Staaten von Amerika}}Veroeffentlichungssort{Journal of Construction Engineering and Management}Art{Journal, Fachzeitschrift}Zielgruppe{Ingeneure, Konstrukteure, Architekten, Facility Manager, Privatsektor{AEC-Hintergrund, Projektmanager}}Thema{Informationsberechnung und Praesentation}Anwendungsfeld{AEC, AEC{Vorkonstruktion, Entwurf, Konstruktion, lifetime}, Facility Management}Methodik{Literaturrezension, Entwurf, Diskussion, Analyse, Bericht}Ergebnis{anwendbar{Framework}, praktisch{Implementation}, theoretisch{Bericht}}},
	doi = {10.1061/(asce)0733-9364(2008)134:7(509)},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\Integrating_Construction_Process_Documentation.pdf:PDF},
	keywords = {Data communication; Information retrieval; Information management; Parameters; Three- dimensional models; Construction management; Documentation},
	publisher = {American Society of Civil Engineers ({ASCE})},
	url = {http://prof.incheon.ac.kr:8082/~uicem/pdf/seminar/090312_1.pdf}
}

@Article{Sacks2010,
	author = {Rafael Sacks and Lauri Koskela and Bhargav A. Dave and Robert Owen},
	title = {Interaction of Lean and Building Information Modeling in Construction},
	journal = {Journal of Construction Engineering and Management},
	year = {2010},
	volume = {136},
	number = {9},
	pages = {968--980},
	month = "sep",
	abstract = {Lean construction and Building Information Modeling are quite different initiatives, but both are having profound impacts on the construction industry.A rigorous analysis of the myriad specific interactions between them indicates that a synergy exists which, if properly understood in theoretical terms,can be exploited to improve construction processes beyond the degree to which it might be improved by application of either of these paradigms independently. Using a matrix that juxtaposes BIM functionalities with prescriptive lean construction principles,fifty-six interactions have been identified, all but four of which represent constructive interaction. Although evidence for the majority of these has been found, the matrix is not considered complete, but rather a framework for research to explore the degree of validity of the interactions. Construction executives, managers, designers and developers of IT systems for construction can also benefit from the framework as an aid to recognizing the potential synergies when planning their lean and BIM adoption strategies.},
	classes = {Veroeffentlichungsland{Israel, UK, Asien{Israel}, Europa{UK, Vereinigtes Koenigreich}}Zielgruppe{Privatsektor{Projektmanager, Ingeneure, Architekten, Konstrukteure}}Veroeffentlichungssort{Journal of Construction Engineering and Management}Art{Journal, Fachzeitschrift}Thema{Interoperabilitaet BIM und Lean}Anwendungsfeld{AEC, AEC{Vorkonstruktion, Entwurf, Konstruktion}}Ergebnis{theoretisch{Framework, Ueberblick, Workflow}}Methodik{Diskussion, Analyse, Entwurf, Literaturrezension}},
	doi = {10.1061/(asce)co.1943-7862.0000203},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\Interaction_of_Lean_and_BIM_in_Construction_plus_Figures.pdf:PDF},
	keywords = {Computer aided design; construction management;information technology(IT);lean construction.},
	publisher = {American Society of Civil Engineers ({ASCE})},
	url = {http://eprints.hud.ac.uk/id/eprint/25835/1/Interaction_of_Lean_and_BIM_in_Construction_plus_Figures.pdf}
}

@Article{Zhang2013,
	author = {Sijie Zhang and Jochen Teizer and Jin-Kook Lee and Charles M. Eastman and Manu Venugopal},
	title = {Building Information Modeling ({BIM}) and Safety: Automatic Safety Checking of Construction Models and Schedules},
	journal = {Automation in Construction},
	year = {2013},
	volume = {29},
	pages = {183--195},
	month = "jan",
	abstract = {Construction safety is a national and worldwide issue. This paper contributes in solving this problem by applying automated safety rule checking to Building Information Models (BIM). Algorithms that automatically analyze a building model to detect safety hazards and suggest preventive measures to users are developed for different cases involving fall related hazards. As BIM is changing the way construction can be approached, the presented work and case studies extend BIM to include automated hazard identification and correction during construction planning and in certain cases, during design. A rule-based engine that utilizes this framework is implemented on top of a commercially available BIM platform to show the feasibility of the approach. As a result, the developed automated safety checking platform informs construction engineers and managers by reporting, why, where, when, and what safety measures are needed for preventing fall-related accidents before construction starts. The safety area reviewed is fall protection. An example case study of such a system is also provided.},
	classes = {Zielgruppe{unspezifisch}Veroeffentlichungsland{Nordamerika{USA, Vereinigte Staaten von Amerika}, Asien{Suedkorea}}Veroeffentlichungssort{Automation in Construction}Art{Journal, Fachzeitschrift}Thema{Gebaeudesicherheit und Fehlermanagement}Anwendungsfeld{Organisation}Ergebnis{praktisch{Implementation, Framework}, theoretisch{Workflow}}Methodik{Entwurf, Fallstudie, Bericht}},
	doi = {10.1016/j.autcon.2012.05.006},
	file = {:C\:\\Users\\Anwender\\OneDrive\\Bachelorarbeit\\Quellen\\BIM_and_Safety.pdf:PDF},
	keywords = {Building Information Model (BIM) Fall protection Construction safety Planning Prevention through Design (PtD) Rule checking Scheduling Simulation},
	publisher = {Elsevier {BV}},
	url = {https://ac.els-cdn.com/S0926580512000799/1-s2.0-S0926580512000799-main.pdf?_tid=36c8c3fc-cd57-11e7-9b11-00000aab0f01&acdnat=1511116192_3640bed436ca142d6e07319799df2ed3}
}

