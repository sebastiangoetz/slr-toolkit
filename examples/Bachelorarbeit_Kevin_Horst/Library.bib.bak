	publisher = {Elsevier {BV}},
	url = {https://ac.els-cdn.com/S0926580513000873/1-s2.0-S0926580513000873-main.pdf?_tid=5225835c-cd6f-11e7-95f1-00000aab0f6c&acdnat=1511126546_5d7a8ba7247e9597d01201f5478330c4}
}

@InProceedings{GregoriusAGegana2017,
