@InProceedings{Boya2014,
	author = {Ji Boya and Qi Zhenqiang and Jin Zhanyong},
	title = {Based on Game Model to Design of Building Information Modeling Application Policy},
	booktitle = {2014 Fifth International Conference on Intelligent Systems Design and Engineering Applications},
	year = {2014},
	month = "jun",
	publisher = {{IEEE}},
	abstract = {At present, it is an out-of-order situation to promote and apply Building Information Modeling (BIM) in China. Although there are many BIM applications in China, those successful experiences were not used to help other novices, so many Chinese scholars called on some effective strategy for BIM use and most of them referred to the economic barriers. In this paper, we summarizes other Chinese scholars suggested for the BIM application in China and try to present what is the most reasonable economic policy, which is inevitably impeded by the result of the existence of externalities. Moreover, the establishment of Dynamic Game Model based on complete information is firstly proposed among enterprises and government. The overall aim of the Model is to help government to promote Economic Policy. It is concluded that Economic incentive effect is the key factor to whether government improve BIM's development. If the incentive effect is obvious, no matter what responses from other people, the government will resolutely promote economic incentive policies. Therefore, we suggest that the government should adopt BIM applications economic incentive mechanism, including "supply side oriented", "carrot and stick", and "Stage-style" for different interest subjects of economic stimulus policy.},
	classes = {Zielgruppe{Oeffentlichkeit und Regierung, Oeffentlicher Sektor{Regierung}}Veroeffentlichungsland{China, Asien{China}}Veroeffentlichungssort{International Conference on Intelligent Systems Design and Engineering Applications}Art{Conference, Konferenz}Thema{Information und Anleitung zu BIM, Anwendung von BIM Praktiken und Software in zukuenftigen Projekten}Anwendungsfeld{unspezifisch}Abstraktionsgrad{aktuell{Ueberblick}, zukunftsorientiert{Implementation}}Methodik{Literaturrezension, Entwurf}Ergebnis{theoretisch{Workflow, Arbeitsfluss}}},
