@INPROCEEDINGS{Anthony2008,
  author = {Anthony, R. and Pelc, M. and Ward, P. and Hawthorne, J. and Pulnah,
	K.},
  title = {A Run-Time Configurable Software Architecture for Self-Managing Systems},
  booktitle = {Autonomic Computing, 2008. ICAC '08. International Conference on},
  year = {2008},
  pages = {207-208},
  month = {June},
  abstract = {This paper describes a highly flexible component architecture, primarily
	designed for automotive control systems, that supports distributed
	dynamically- configurable context-aware behaviour. The architecture
	enforces a separation of design-time and run-time concerns, enabling
	almost all decisions concerning runtime composition and adaptation
	to be deferred beyond deployment. Dynamic context management contributes
	to flexibility. The architecture is extensible, and can embed potentially
	many different self-management decision technologies simultaneously.
	The mechanism that implements the run-time configuration has been
	designed to be very robust, automatically and silently handling problems
	arising from the evaluation of self- management logic and ensuring
	that in the worst case the dynamic aspects of the system collapse
	down to static behavior in totally predictable ways.},
